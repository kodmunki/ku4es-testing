{
  "name": "@kodmunki/ku4es-testing",
  "version": "1.2.2",
  "description": "kodmunki Utilities for ECMAScript Testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/kodmunki/ku4es-testing"
  },
  "license": "MIT",
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "scripts": {
    "reload": "rm -rf ./node_modules && rm -f ./package-lock.json && npm install",
    "test": "mocha -R spec --require babel-register --recursive ./test",
    "test-file": "mocha -R spec --require babel-register",
    "test-cov": "cross-env NODE_ENV=test nyc npm test",
    "test-cov-d": "open -a 'Google Chrome' ./coverage/lcov-report/index.html",
    "lint": "eslint './src/**/*.js' './test/**/*.js'",
    "lint-fix": "eslint --fix './src/**/*.js' './test/**/*.js'",
    "check": "npm run lint && npm run test-cov",
    "check-d": "npm run test-cov-d",
    "build": "shx mkdir -p dist && babel src -d dist --source-maps inline",
    "prepare": "npm run build"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "moxios": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.0",
    "shx": "^0.2.2"
  },
  "files": [
    "dist"
  ],
  "main": "dist",
  "bugs": {
    "url": "https://github.com/kodmunki/ku4es-testing/issues"
  },
  "homepage": "https://github.com/kodmunki/ku4es-testing#readme",
  "author": "kodmunki"
}
